{
    "name": "rubenvanerk/competition-parser-lumen",
    "description": "A lifesaving competition parser",
    "keywords": [
        "lifesaving",
        "competitions"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",

        "ext-fileinfo": "*",
        "cviebrock/eloquent-sluggable": "^8.0",
        "laravel/ui": "^3.2",
        "league/csv": "^9.6",
        "league/flysystem-aws-s3-v3": "^1.0",
        "leonverschuren/lenex": "dev-master",
        "smalot/pdfparser": "dev-master",
        "spatie/laravel-medialibrary": "^8.3",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",

        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.7",
        "friendsofphp/php-cs-fixer": "^2.16",
        "nunomaduro/larastan": "^0.6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "vendor/bin/phpunit",
            "vendor/bin/phpstan analyse",
            "vendor/bin/php-cs-fixer fix --dry-run"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rubenvanerk/pdfparser"
        },
        {
            "type": "vcs",
            "url": "https://github.com/leonverschuren/lenex-php"
        }
    ]
}
