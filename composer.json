{
    "name": "rubenvanerk/competition-parser-lumen",
    "description": "A lifesaving competition parser",
    "keywords": [
        "lifesaving",
        "competitions"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "ext-fileinfo": "*",
        "bref/bref": "^0.5.27",
        "cviebrock/eloquent-sluggable": "^7.0",
        "fideloper/proxy": "^4.4",
        "laravel/framework": "^7.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "leonverschuren/lenex": "dev-master",
        "smalot/pdfparser": "dev-feature/remove-tcpdf-lib",
        "symfony/yaml": "^5.0",
        "fruitcake/laravel-cors": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/larastan": "^0.6.0",
        "phpunit/phpunit": "^9",
        "facade/ignition": "^2.0",
        "nunomaduro/collision": "^4.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "vendor/bin/phpunit",
            "vendor/bin/phpstan analyse",
            "vendor/bin/php-cs-fixer fix --dry-run"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rubenvanerk/pdfparser"
        },
        {
            "type": "vcs",
            "url": "https://github.com/leonverschuren/lenex-php"
        }
    ]
}
